{
	"name": "wrms",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.0.0",
		"@tsconfig/svelte": "^3.0.0",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier": "^2.8.3",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.0"
	},
	"prettier": {
		"useTabs": true,
		"singleQuote": true,
		"trailingComma": "none",
		"printWidth": 100,
		"plugins": [
			"prettier-plugin-svelte"
		],
		"pluginSearchDirs": [
			"."
		],
		"overrides": [
			{
				"files": "*.svelte",
				"options": {
					"parser": "svelte"
				}
			}
		]
	}
}
